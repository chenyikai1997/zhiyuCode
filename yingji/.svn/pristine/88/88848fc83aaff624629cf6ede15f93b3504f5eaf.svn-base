/* 声明字体*/
@font-face {
  font-family: electronicFont;
  src: url(../fonts/DS-DIGIT.TTF);
}
.main-container {
  margin: 0 auto;
  margin-top: 0.375rem;
  display: flex;
  justify-content: space-between;
  width: 98%;
  height: 90vh;
}
.main-container .left-container {
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 25%;
  height: 100%;
}
.main-container .left-container .title {
  padding-left: 0.1875rem;
  width: 50%;
  font-size: 0.225rem;
  color: #fff;
  background-image: url(../images/indexTitle-bg.png);
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.main-container .left-container .top-box {
  position: relative;
  padding: 0 0.25rem;
  display: flex;
  align-items: center;
  width: 90%;
  height: 20%;
  background-color: #2947db38;
  background-size: 10% 100%;
  background-repeat: no-repeat;
  border-radius: 0.15rem;
}
.main-container .left-container .top-box::before {
  position: absolute;
  top: 0;
  left: 0;
  width: 0.375rem;
  height: 0.375rem;
  background-image: url(../images/box-leftUp.png);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  content: "";
}
.main-container .left-container .top-box::after {
  position: absolute;
  top: 0;
  right: 0;
  width: 0.375rem;
  height: 0.375rem;
  background-image: url(../images/box-rightUp.png);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  content: "";
}
.main-container .left-container .top-box .panel-footer {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
}
.main-container .left-container .top-box .panel-footer::before {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 0.375rem;
  height: 0.375rem;
  background-image: url(../images/box-leftBottom.png);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  content: "";
}
.main-container .left-container .top-box .panel-footer::after {
  position: absolute;
  bottom: 0;
  right: 0;
  width: 0.375rem;
  height: 0.375rem;
  background-image: url(../images/box-rightBottom.png);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  content: "";
}
.main-container .left-container .top-box .hkLogo {
  margin-right: 0.25rem;
